\cf2 #================================ Logging ======================================\cf3 \
\cf2 # There are four options for the log output: file, stderr, syslog, eventlog\cf3 \
\cf2 # The file output is the default.\cf3 \
\
\cf2 # Sets log level. The default log level is info.\cf3 \
\cf2 # Available log levels are: error, warning, info, debug\cf3 \
\cf2 #logging.level: info\cf3 \
\
\cf2 # Enable debug output for selected components. To enable all selectors use ["*"]\cf3 \
\cf2 # Other available selectors are "beat", "publish", "service"\cf3 \
\cf2 # Multiple selectors can be chained.\cf3 \
\cf2 #logging.selectors: [ ]\cf3 \
\
\cf2 # Send all logging output to stderr. The default is false.\cf3 \
\cf2 #logging.to_stderr: false\cf3 \
\
\cf2 # Send all logging output to syslog. The default is false.\cf3 \
\cf2 #logging.to_syslog: false\cf3 \
\
\cf2 # Send all logging output to Windows Event Logs. The default is false.\cf3 \
\cf2 #logging.to_eventlog: false\cf3 \
\
\cf2 # If enabled, Filebeat periodically logs its internal metrics that have changed\cf3 \
\cf2 # in the last period. For each metric that changed, the delta from the value at\cf3 \
\cf2 # the beginning of the period is logged. Also, the total values for\cf3 \
\cf2 # all non-zero internal metrics are logged on shutdown. The default is true.\cf3 \
\cf2 #logging.metrics.enabled: true\cf3 \
\
\cf2 # The period after which to log the internal metrics. The default is 30s.\cf3 \
\cf2 #logging.metrics.period: 30s\cf3 \
\
\cf2 # Logging to rotating files. Set logging.to_files to false to disable logging to\cf3 \
\cf2 # files.\cf3 \
logging.to_files: true\
logging.files:\
\cf2   # Configure the path where the logs are written. The default is the logs directory\cf3 \
\cf2   # under the home path (the binary location).\cf3 \
\cf2   #path: /var/log/filebeat\cf3 \
\
\cf2   # The name of the files where the logs are written to.\cf3 \
\cf2   #name: filebeat\cf3 \
\
\cf2   # Configure log file size limit. If limit is reached, log file will be\cf3 \
\cf2   # automatically rotated\cf3 \
\cf2   #rotateeverybytes: 10485760 # = 10MB\cf3 \
\cf2 # Number of rotated log files to keep. Oldest files will be deleted first.\cf3 \
\cf2   #keepfiles: 7\cf3 \
\
\cf2   # The permissions mask to apply when rotating log files. The default value is 0600.\cf3 \
\cf2   # Must be a valid Unix-style file permissions mask expressed in octal notation.\cf3 \
\cf2   #permissions: 0600\cf3 \
\
\cf2   # Enable log file rotation on time intervals in addition to size-based rotation.\cf3 \
\cf2   # Intervals must be at least 1s. Values of 1m, 1h, 24h, 7*24h, 30*24h, and 365*24h\cf3 \
\cf2   # are boundary-aligned with minutes, hours, days, weeks, months, and years as\cf3 \
\cf2   # reported by the local system clock. All other intervals are calculated from the\cf3 \
\cf2   # Unix epoch. Defaults to disabled.\cf3 \
\cf2   #interval: 0\cf3 \
\
\cf2   # Rotate existing logs on startup rather than appending to the existing\cf3 \
\cf2   # file. Defaults to true.\cf3 \
\cf2   # rotateonstartup: true\cf3 \
\
\cf2 # Set to true to log messages in JSON format.\cf3 \
\cf2 #logging.json: false\cf3 \
\
