\cf2 #================================ General ======================================\cf3 \
\cf2 # The name of the shipper that publishes the network data. It can be used to group\cf3 \
\cf2 # all the transactions sent by a single shipper in the web interface.\cf3 \
\cf2 # If this options is not defined, the hostname is used.\cf3 \
\cf2 #name:\cf3 \
\
\cf2 # The tags of the shipper are included in their own field with each\cf3 \
\cf2 # transaction published. Tags make it easy to group servers by different\cf3 \
\cf2 # logical properties.\cf3 \
\cf2 #tags: ["service-X", "web-tier"]\cf3 \
\
\cf2 # Optional fields that you can specify to add additional information to the\cf3 \
\cf2 # output. Fields can be scalar values, arrays, dictionaries, or any nested\cf3 \
\cf2 # combination of these.\cf3 \
\cf2 #fields:\cf3 \
\cf2 #  env: staging\cf3 \
\
\cf2 # If this option is set to true, the custom fields are stored as top-level\cf3 \
\cf2 # fields in the output document instead of being grouped under a fields\cf3 \
\cf2 # sub-dictionary. Default is false.\cf3 \
\cf2 #fields_under_root: false\cf3 \
\
\cf2 # Internal queue configuration for buffering events to be published.\cf3 \
\cf2 #queue:\cf3 \
\cf2   # Queue type by name (default 'mem')\cf3 \
\cf2   # The memory queue will present all available events (up to the outputs\cf3 \
\cf2   # bulk_max_size) to the output, the moment the output is ready to server\cf3 \
\cf2   # another batch of events.\cf3 \
\cf2   #mem:\cf3 \
\cf2     # Max number of events the queue can buffer.\cf3 \
\cf2     #events: 4096\cf3 \
\
\cf2     # Hints the minimum number of events stored in the queue,\cf3 \
\cf2     # before providing a batch of events to the outputs.\cf3 \
\cf2     # The default value is set to 2048.\cf3 \
\cf2     # A value of 0 ensures events are immediately available\cf3 \
\cf2     # to be sent to the outputs.\cf3 \
\cf2     #flush.min_events: 2048\cf3 \
\
\cf2     # Maximum duration after which events are available to the outputs,\cf3 \
\cf2     # if the number of events stored in the queue is < `flush.min_events`.\cf3 \
\cf2     #flush.timeout: 1s\cf3 \
\
\cf2   # The spool queue will store events in a local spool file, before\cf3 \
\cf2   # forwarding the events to the outputs.\cf3 \
\cf2 # Beta: spooling to disk is currently a beta feature. Use with care.\cf3 \
\cf2   #\cf3 \
\cf2   # The spool file is a circular buffer, which blocks once the file/buffer is full.\cf3 \
\cf2   # Events are put into a write buffer and flushed once the write buffer\cf3 \
\cf2   # is full or the flush_timeout is triggered.\cf3 \
\cf2   # Once ACKed by the output, events are removed immediately from the queue,\cf3 \
\cf2   # making space for new events to be persisted.\cf3 \
\cf2   #spool:\cf3 \
\cf2     # The file namespace configures the file path and the file creation settings.\cf3 \
\cf2     # Once the file exists, the `size`, `page_size` and `prealloc` settings\cf3 \
\cf2     # will have no more effect.\cf3 \
\cf2     #file:\cf3 \
\cf2       # Location of spool file. The default value is $\{path.data\}/spool.dat.\cf3 \
\cf2       #path: "$\{path.data\}/spool.dat"\cf3 \
\
\cf2       # Configure file permissions if file is created. The default value is 0600.\cf3 \
\cf2       #permissions: 0600\cf3 \
\
\cf2       # File size hint. The spool blocks, once this limit is reached. The default value is 100 MiB.\cf3 \
\cf2       #size: 100MiB\cf3 \
\
\cf2       # The files page size. A file is split into multiple pages of the same size. The default value is 4KiB.\cf3 \
\cf2       #page_size: 4KiB\cf3 \
\
\cf2       # If prealloc is set, the required space for the file is reserved using\cf3 \
\cf2       # truncate. The default value is true.\cf3 \
\cf2       #prealloc: true\cf3 \
\
\cf2     # Spool writer settings\cf3 \
\cf2     # Events are serialized into a write buffer. The write buffer is flushed if:\cf3 \
\cf2     # - The buffer limit has been reached.\cf3 \
\cf2     # - The configured limit of buffered events is reached.\cf3 \
\cf2     # - The flush timeout is triggered.\cf3 \
\cf2     #write:\cf3 \
\cf2       # Sets the write buffer size.\cf3 \
\cf2       #buffer_size: 1MiB\cf3 \
\
\cf2       # Maximum duration after which events are flushed if the write buffer\cf3 \
\cf2       # is not full yet. The default value is 1s.\cf3 \
\cf2       #flush.timeout: 1s\cf3 \
\
\cf2       # Number of maximum buffered events. The write buffer is flushed once the\cf3 \
\cf2       # limit is reached.\cf3 \
\cf2       #flush.events: 16384\cf3 \
\
\cf2  # Configure the on-disk event encoding. The encoding can be changed\cf3 \
\cf2       # between restarts.\cf3 \
\cf2       # Valid encodings are: json, ubjson, and cbor.\cf3 \
\cf2       #codec: cbor\cf3 \
\cf2     #read:\cf3 \
\cf2       # Reader flush timeout, waiting for more events to become available, so\cf3 \
\cf2       # to fill a complete batch as required by the outputs.\cf3 \
\cf2       # If flush_timeout is 0, all available events are forwarded to the\cf3 \
\cf2       # outputs immediately.\cf3 \
\cf2       # The default value is 0s.\cf3 \
\cf2       #flush.timeout: 0s\cf3 \
\
\cf2 # Sets the maximum number of CPUs that can be executing simultaneously. The\cf3 \
\cf2 # default is the number of logical CPUs available in the system.\cf3 \
\cf2 #max_procs:\cf3 \
\
\cf2 #================================ Processors ===================================\cf3 \
\
\cf2 # Processors are used to reduce the number of fields in the exported event or to\cf3 \
\cf2 # enhance the event with external metadata. This section defines a list of\cf3 \
\cf2 # processors that are applied one by one and the first one receives the initial\cf3 \
\cf2 # event:\cf3 \
\cf2 #\cf3 \
\cf2 #   event -> filter1 -> event1 -> filter2 ->event2 ...\cf3 \
\cf2 #\cf3 \
\cf2 # The supported processors are drop_fields, drop_event, include_fields,\cf3 \
\cf2 # decode_json_fields, and add_cloud_metadata.\cf3 \
\cf2 #\cf3 \
\cf2 # For example, you can use the following processors to keep the fields that\cf3 \
\cf2 # contain CPU load percentages, but remove the fields that contain CPU ticks\cf3 \
\cf2 # values:\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- include_fields:\cf3 \
\cf2 #    fields: ["cpu"]\cf3 \
\cf2 #- drop_fields:\cf3 \
\cf2 #    fields: ["cpu.user", "cpu.system"]\cf3 \
\cf2 #\cf3 \
\cf2 # The following example drops the events that have the HTTP response code 200:\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- drop_event:\cf3 \
\cf2 #    when:\cf3 \
\cf2 #       equals:\cf3 \
\cf2 #           http.code: 200\cf3 \
\
\cf2 # The following example renames the field a to b:\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- rename:\cf3 \
\cf2 #    fields:\cf3 \
\cf2 #       - from: "a"\cf3 \
\cf2 #         to: "b"\cf3 \
\cf2 #\cf3 \
\cf2 # The following example tokenizes the string into fields:\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- dissect:\cf3 \
\cf2 #    tokenizer: "%\{key1\} - %\{key2\}"\cf3 \
\cf2 #    field: "message"\cf3 \
\cf2 #    target_prefix: "dissect"\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with metadata from the cloud\cf3 \
\cf2 # provider about the host machine. It works on EC2, GCE, DigitalOcean,\cf3 \
\cf2 # Tencent Cloud, and Alibaba Cloud.\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_cloud_metadata: ~\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with the machine's local time zone\cf3 \
\cf2 # offset from UTC.\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_locale:\cf3 \
\cf2 #    format: offset\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with docker metadata, it matches\cf3 \
\cf2 # given fields to an existing container id and adds info from that container:\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_docker_metadata:\cf3 \
\cf2 #    host: "unix:///var/run/docker.sock"\cf3 \
\cf2 #    match_fields: ["system.process.cgroup.id"]\cf3 \
\cf2 #    match_pids: ["process.pid", "process.ppid"]\cf3 \
\cf2 #    match_source: true\cf3 \
\cf2 #    match_source_index: 4\cf3 \
\cf2 #    match_short_id: false\cf3 \
\cf2 #    cleanup_timeout: 60\cf3 \
\cf2 #    labels.dedot: false\cf3 \
\cf2 #    # To connect to Docker over TLS you must specify a client and CA certificate.\cf3 \
\cf2 #    #ssl:\cf3 \
\cf2 #    #  certificate_authority: "/etc/pki/root/ca.pem"\cf3 \
\cf2 #    #  certificate:           "/etc/pki/client/cert.pem"\cf3 \
\cf2 #    #  key:\cf3                    \cf2 "/etc/pki/client/cert.key"\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with docker metadata, it matches\cf3 \
\cf2 # container id from log path available in `source` field (by default it expects\cf3 \
\cf2 # it to be /var/lib/docker/containers/*/*.log).\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_docker_metadata: ~\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with host metadata.\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_host_metadata:\cf3 \
\cf2 #   netinfo.enabled: false\cf3 \
\cf2 #\cf3 \
\cf2 # The following example enriches each event with process metadata using\cf3 \
\cf2 # process IDs included in the event.\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- add_process_metadata:\cf3 \
\cf2 #    match_pids: ["system.process.ppid"]\cf3 \
\cf2 #    target: system.process.parent\cf3 \
\cf2 #\cf3 \
\cf2 # The following example decodes fields containing JSON strings\cf3 \
\cf2 # and replaces the strings with valid JSON objects.\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- decode_json_fields:\cf3 \
\cf2 #    fields: ["field1", "field2", ...]\cf3 \
\cf2 #    process_array: false\cf3 \
\cf2 #    max_depth: 1\cf3 \
\cf2 #    target: ""\cf3 \
\cf2 #    overwrite_keys: false\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- decompress_gzip_field:\cf3 \
\cf2 #    from: "field1"\cf3 \
\cf2 #    to: "field2"\cf3 \
\cf2 #    ignore_missing: false\cf3 \
\cf2 #    fail_on_error: true\cf3 \
\cf2 #\cf3 \
\cf2 # The following example copies the value of message to message_copied# The following example copies the value of message to message_copied\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
\cf2 #- copy_fields:\cf3 \
\cf2 #    fields:\cf3 \
\cf2 #        - from: message\cf3 \
\cf2 #          to: message_copied\cf3 \
\cf2 #    fail_on_error: true\cf3 \
\cf2 #    ignore_missing: false\cf3 \
\cf2 #\cf3 \
\cf2 # The following example truncates the value of message to 1024 bytes\cf3 \
\cf2 #\cf3 \
\cf2 #processors:\cf3 \
