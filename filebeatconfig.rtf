\cf2 #============================= Elastic Cloud ==================================\cf3 \
\
\cf2 # These settings simplify using Filebeat with the Elastic Cloud (https://cloud.elastic.co/).\cf3 \
\
\cf2 # The cloud.id setting overwrites the `output.elasticsearch.hosts` and\cf3 \
\cf2 # `setup.kibana.host` options.\cf3 \
\cf2 # You can find the `cloud.id` in the Elastic Cloud web UI.\cf3 \
\cf2 #cloud.id:\cf3 \
\
\cf2 # The cloud.auth setting overwrites the `output.elasticsearch.username` and\cf3 \
\cf2 # `output.elasticsearch.password` settings. The format is `<user>:<pass>`.\cf3 \
\cf2 #cloud.auth:\cf3 \
\
\cf2 #================================ Outputs ======================================\cf3 \
\
\cf2 # Configure what output to use when sending the data collected by the beat.\cf3 \
\
\cf2 #-------------------------- Elasticsearch output -------------------------------\cf3 \
output.elasticsearch:\
\cf2   # Boolean flag to enable or disable the output module.\cf3 \
\cf2   #enabled: true\cf3 \
\
\cf2   # Array of hosts to connect to.\cf3 \
\cf2   # Scheme and port can be left out and will be set to the default (http and 9200)\cf3 \
\cf2   # In case you specify and additional path, the scheme is required: http://localhost:9200/path\cf3 \
\cf2   # IPv6 addresses should always be defined as: https://[2001:db8::1]:9200\cf3 \
  hosts: ["10.2.0.4:9200"]\
  username: "elastic"\
  password: "changeme"\
\
\cf2   # Set gzip compression level.\cf3 \
\cf2   #compression_level: 0\cf3 \
\
\cf2   # Configure escaping HTML symbols in strings.\cf3 \
\cf2   #escape_html: false\cf3 \
\
\cf2   # Optional protocol and basic auth credentials.\cf3 \
\cf2   #protocol: "https"\cf3 \
\cf2   #username: "elastic"\cf3 \
\cf2   #password: "changeme"\cf3 \
\
\cf2   # Dictionary of HTTP parameters to pass within the URL with index operations.\cf3 \
\cf2   #parameters:\cf3 \
\cf2     #param1: value1\cf3 \
\cf2     #param2: value2\cf3 \
\
\cf2   # Number of workers per Elasticsearch host.\cf3 \
\cf2   #worker: 1\cf3 \
\
\cf2   # Optional index name. The default is "filebeat" plus date\cf3 \
\cf2   # and generates [filebeat-]YYYY.MM.DD keys.\cf3 \
\cf2   # In case you modify this pattern you must update setup.template.name and setup.template.pattern accordingly.\cf3 \
\cf2   #index: "filebeat-%\{[agent.version]\}-%\{+yyyy.MM.dd\}"\cf3 \
\
\cf2  # Optional ingest node pipeline. By default no pipeline will be used.\cf3 \
\cf2   #pipeline: ""\cf3 \
\
\cf2   # Optional HTTP path\cf3 \
\cf2   #path: "/elasticsearch"\cf3 \
\
\cf2   # Custom HTTP headers to add to each request\cf3 \
\cf2   #headers:\cf3 \
\cf2   #  X-My-Header: Contents of the header\cf3 \
\
\cf2   # Proxy server URL\cf3 \
\cf2   #proxy_url: http://proxy:3128\cf3 \
\
\cf2   # Whether to disable proxy settings for outgoing connections. If true, this\cf3 \
\cf2   # takes precedence over both the proxy_url field and any environment settings\cf3 \
\cf2   # (HTTP_PROXY, HTTPS_PROXY). The default is false.\cf3 \
\cf2   #proxy_disable: false\cf3 \
\
\cf2   # The number of times a particular Elasticsearch index operation is attempted. If\cf3 \
\cf2   # the indexing operation doesn't succeed after this many retries, the events are\cf3 \
\cf2   # dropped. The default is 3.\cf3 \
\cf2   #max_retries: 3\cf3 \
\
\cf2   # The maximum number of events to bulk in a single Elasticsearch bulk API index request.\cf3 \
\cf2   # The default is 50.\cf3 \
\cf2   #bulk_max_size: 50\cf3 \
\
\cf2   # The number of seconds to wait before trying to reconnect to Elasticsearch\cf3 \
\cf2   # after a network error. After waiting backoff.init seconds, the Beat\cf3 \
\cf2   # tries to reconnect. If the attempt fails, the backoff timer is increased\cf3 \
\cf2   # exponentially up to backoff.max. After a successful connection, the backoff\cf3 \
\cf2   # timer is reset. The default is 1s.\cf3 \
\cf2   #backoff.init: 1s\cf3 \
\
\cf2   # The maximum number of seconds to wait before attempting to connect to\cf3 \
\cf2   # Elasticsearch after a network error. The default is 60s.\cf3 \
\cf2   #backoff.max: 60s\cf3 \
\
\cf2   # Configure HTTP request timeout before failing a request to Elasticsearch.\cf3 \
\cf2   #timeout: 90\cf3 \
\
\cf2   # Use SSL settings for HTTPS.\cf3 \
\cf2   #ssl.enabled: true\cf3 \
\
\cf2   # Configure SSL verification mode. If `none` is configured, all server hosts # and certificates will be accepted. In this mode, SSL-based connections are\cf3 \
\cf2   # susceptible to man-in-the-middle attacks. Use only for testing. Default is\cf3 \
\cf2   # `full`.\cf3 \
\cf2   #ssl.verification_mode: full\cf3 \
\
\cf2   # List of supported/valid TLS versions. By default all TLS versions from 1.0 up to\cf3 \
\cf2   # 1.2 are enabled.\cf3 \
\cf2   #ssl.supported_protocols: [TLSv1.0, TLSv1.1, TLSv1.2]\cf3 \
\
\cf2   # List of root certificates for HTTPS server verifications\cf3 \
\cf2   #ssl.certificate_authorities: ["/etc/pki/root/ca.pem"]\cf3 \
\
\cf2   # Certificate for SSL client authentication\cf3 \
\cf2   #ssl.certificate: "/etc/pki/client/cert.pem"\cf3 \
\
\cf2   # Client certificate key\cf3 \
\cf2   #ssl.key: "/etc/pki/client/cert.key"\cf3 \
\
\cf2   # Optional passphrase for decrypting the certificate key.\cf3 \
\cf2   #ssl.key_passphrase: ''\cf3 \
\
\cf2   # Configure cipher suites to be used for SSL connections\cf3 \
\cf2   #ssl.cipher_suites: []\cf3 \
\
\cf2   # Configure curve types for ECDHE-based cipher suites\cf3 \
\cf2   #ssl.curve_types: []\cf3 \
\
\cf2   # Configure what types of renegotiation are supported. Valid options are\cf3 \
\cf2   # never, once, and freely. Default is never.\cf3 \
\cf2   #ssl.renegotiation: never\cf3 \
\
\cf2 #----------------------------- Logstash output ---------------------------------\cf3 \
\cf2 #output.logstash:\cf3 \
\cf2   # Boolean flag to enable or disable the output module.\cf3 \
\cf2   #enabled: true\cf3 \
\
\cf2   # The Logstash hosts\cf3 \
\cf2   #hosts: ["localhost:5044"]\cf3 \
\
\cf2   # Number of workers per Logstash host.\cf3 \
\cf2   #worker: 1\cf3 \
\
\cf2   # Set gzip compression level.\cf3 \
\cf2   #compression_level: 3\cf3 \
\cf2  # Configure escaping HTML symbols in strings.\cf3 \
\cf2   #escape_html: false\cf3 \
\
\cf2   # Optional maximum time to live for a connection to Logstash, after which the\cf3 \
\cf2   # connection will be re-established.  A value of `0s` (the default) will\cf3 \
\cf2   # disable this feature.\cf3 \
\cf2   #\cf3 \
\cf2   # Not yet supported for async connections (i.e. with the "pipelining" option set)\cf3 \
\cf2   #ttl: 30s\cf3 \
\
\cf2   # Optionally load-balance events between Logstash hosts. Default is false.\cf3 \
\cf2   #loadbalance: false\cf3 \
\
\cf2   # Number of batches to be sent asynchronously to Logstash while processing\cf3 \
\cf2   # new batches.\cf3 \
\cf2   #pipelining: 2\cf3 \
\
\cf2   # If enabled only a subset of events in a batch of events is transferred per\cf3 \
\cf2   # transaction.  The number of events to be sent increases up to `bulk_max_size`\cf3 \
\cf2   # if no error is encountered.\cf3 \
\cf2   #slow_start: false\cf3 \
\
\cf2   # The number of seconds to wait before trying to reconnect to Logstash\cf3 \
\cf2   # after a network error. After waiting backoff.init seconds, the Beat\cf3 \
\cf2   # tries to reconnect. If the attempt fails, the backoff timer is increased\cf3 \
\cf2   # exponentially up to backoff.max. After a successful connection, the backoff\cf3 \
\cf2   # timer is reset. The default is 1s.\cf3 \
\cf2   #backoff.init: 1s\cf3 \
\
\cf2   # The maximum number of seconds to wait before attempting to connect to\cf3 \
\cf2   # Logstash after a network error. The default is 60s.\cf3 \
\cf2   #backoff.max: 60s\cf3 \
\
\cf2   # Optional index name. The default index name is set to filebeat\cf3 \
\cf2   # in all lowercase.\cf3 \
\cf2   #index: 'filebeat'\cf3 \
\
\cf2   # SOCKS5 proxy server URL\cf3 \
\cf2   #proxy_url: socks5://user:password@socks5-server:2233\cf3 \
\
\cf2   # Resolve names locally when using a proxy server. Defaults to false.\cf3 \
\cf2   #proxy_use_local_resolver: false\cf3 \
\
\cf2   # Enable SSL support. SSL is automatically enabled if any SSL setting is set.\cf3 \
\cf2   #ssl.enabled: true\cf3 \
\cf2  # Configure SSL verification mode. If `none` is configured, all server hosts\cf3 \
\cf2   # and certificates will be accepted. In this mode, SSL based connections are\cf3 \
\cf2   # susceptible to man-in-the-middle attacks. Use only for testing. Default is\cf3 \
\cf2   # `full`.\cf3 \
\cf2   #ssl.verification_mode: full\cf3 \
\
\cf2   # List of supported/valid TLS versions. By default all TLS versions from 1.0 up to\cf3 \
\cf2   # 1.2 are enabled.\cf3 \
\cf2   #ssl.supported_protocols: [TLSv1.0, TLSv1.1, TLSv1.2]\cf3 \
\
\cf2   # Optional SSL configuration options. SSL is off by default.\cf3 \
\cf2   # List of root certificates for HTTPS server verifications\cf3 \
\cf2   #ssl.certificate_authorities: ["/etc/pki/root/ca.pem"]\cf3 \
\
\cf2   # Certificate for SSL client authentication\cf3 \
\cf2   #ssl.certificate: "/etc/pki/client/cert.pem"\cf3 \
\
\cf2   # Client certificate key\cf3 \
\cf2   #ssl.key: "/etc/pki/client/cert.key"\cf3 \
\
\cf2   # Optional passphrase for decrypting the Certificate Key.\cf3 \
\cf2   #ssl.key_passphrase: ''\cf3 \
\
\cf2   # Configure cipher suites to be used for SSL connections\cf3 \
\cf2   #ssl.cipher_suites: []\cf3 \
\
\cf2   # Configure curve types for ECDHE-based cipher suites\cf3 \
\cf2   #ssl.curve_types: []\cf3 \
\
\cf2   # Configure what types of renegotiation are supported. Valid options are\cf3 \
\cf2   # never, once, and freely. Default is never.\cf3 \
\cf2   #ssl.renegotiation: never\cf3 \
\
\cf2   # The number of times to retry publishing an event after a publishing failure.\cf3 \
\cf2   # After the specified number of retries, the events are typically dropped.\cf3 \
\cf2   # Some Beats, such as Filebeat and Winlogbeat, ignore the max_retries setting\cf3 \
\cf2   # and retry until all events are published.  Set max_retries to a value less\cf3 \
\cf2   # than 0 to retry until all events are published. The default is 3.\cf3 \
\cf2   #max_retries: 3\cf3 \
\
\cf2   # The maximum number of events to bulk in a single Logstash request. The\cf3 \
\cf2   # default is 2048.\cf3 \
\cf2   #bulk_max_size: 2048\cf3 \
\
\cf2   # The number of seconds to wait for responses from the Logstash server before#timeout: 30s\cf3 \
\
\cf2 #------------------------------- Kafka output ----------------------------------\cf3 \
\cf2 #output.kafka:\cf3 \
\cf2   # Boolean flag to enable or disable the output module.\cf3 \
\cf2   #enabled: true\cf3 \
\
\cf2   # The list of Kafka broker addresses from which to fetch the cluster metadata.\cf3 \
\cf2   # The cluster metadata contain the actual Kafka brokers events are published\cf3 \
\cf2   # to.\cf3 \
\cf2   #hosts: ["localhost:9092"]\cf3 \
\
\cf2   # The Kafka topic used for produced events. The setting can be a format string\cf3 \
\cf2   # using any event field. To set the topic from document type use `%\{[type]\}`.\cf3 \
\cf2   #topic: beats\cf3 \
\
\cf2   # The Kafka event key setting. Use format string to create a unique event key.\cf3 \
\cf2   # By default no event key will be generated.\cf3 \
\cf2   #key: ''\cf3 \
\
\cf2   # The Kafka event partitioning strategy. Default hashing strategy is `hash`\cf3 \
\cf2   # using the `output.kafka.key` setting or randomly distributes events if\cf3 \
\cf2   # `output.kafka.key` is not configured.\cf3 \
\cf2   #partition.hash:\cf3 \
\cf2     # If enabled, events will only be published to partitions with reachable\cf3 \
\cf2     # leaders. Default is false.\cf3 \
\cf2     #reachable_only: false\cf3 \
\
\cf2     # Configure alternative event field names used to compute the hash value.\cf3 \
\cf2     # If empty `output.kafka.key` setting will be used.\cf3 \
\cf2     # Default value is empty list.\cf3 \
\cf2     #hash: []\cf3 \
\
\cf2   # Authentication details. Password is required if username is set.\cf3 \
\cf2   #username: ''\cf3 \
\cf2   #password: ''\cf3 \
\
\cf2   # Kafka version Filebeat is assumed to run against. Defaults to the "1.0.0".\cf3 \
\cf2   #version: '1.0.0'\cf3 \
\
\cf2   # Configure JSON encoding\cf3 \
\cf2   #codec.json:\cf3 \
\cf2     # Pretty-print JSON event\cf3 \
\cf2     #pretty: false\cf3 \
\
\cf2 # Configure escaping HTML symbols in strings.\cf3 \
\cf2     #escape_html: false\cf3 \
\
\cf2   # Metadata update configuration. Metadata contains leader information\cf3 \
\cf2   # used to decide which broker to use when publishing.\cf3 \
\cf2   #metadata:\cf3 \
\cf2     # Max metadata request retry attempts when cluster is in middle of leader\cf3 \
\cf2     # election. Defaults to 3 retries.\cf3 \
\cf2     #retry.max: 3\cf3 \
\
\cf2     # Wait time between retries during leader elections. Default is 250ms.\cf3 \
\cf2     #retry.backoff: 250ms\cf3 \
\
\cf2     # Refresh metadata interval. Defaults to every 10 minutes.\cf3 \
\cf2     #refresh_frequency: 10m\cf3 \
\
\cf2     # Strategy for fetching the topics metadata from the broker. Default is false.\cf3 \
\cf2     #full: false\cf3 \
\
\cf2   # The number of concurrent load-balanced Kafka output workers.\cf3 \
\cf2   #worker: 1\cf3 \
\
\cf2   # The number of times to retry publishing an event after a publishing failure.\cf3 \
\cf2   # After the specified number of retries, events are typically dropped.\cf3 \
\cf2   # Some Beats, such as Filebeat, ignore the max_retries setting and retry until\cf3 \
\cf2   # all events are published.  Set max_retries to a value less than 0 to retry\cf3 \
\cf2   # until all events are published. The default is 3.\cf3 \
\cf2   #max_retries: 3\cf3 \
\
\cf2   # The maximum number of events to bulk in a single Kafka request. The default\cf3 \
\cf2   # is 2048.\cf3 \
\cf2   #bulk_max_size: 2048\cf3 \
\
\cf2   # Duration to wait before sending bulk Kafka request. 0 is no delay. The default\cf3 \
\cf2   # is 0.\cf3 \
\cf2   #bulk_flush_frequency: 0s\cf3 \
\
\cf2   # The number of seconds to wait for responses from the Kafka brokers before\cf3 \
\cf2   # timing out. The default is 30s.\cf3 \
\cf2   #timeout: 30s\cf3 \
\
\cf2   # The maximum duration a broker will wait for number of required ACKs. The\cf3 \
\cf2   # default is 10s.\cf3 \
\cf2  # The number of messages buffered for each Kafka broker. The default is 256.\cf3 \
\cf2   #channel_buffer_size: 256\cf3 \
\
\cf2   # The keep-alive period for an active network connection. If 0s, keep-alives\cf3 \
\cf2   # are disabled. The default is 0 seconds.\cf3 \
\cf2   #keep_alive: 0\cf3 \
\
\cf2   # Sets the output compression codec. Must be one of none, snappy and gzip. The\cf3 \
\cf2   # default is gzip.\cf3 \
\cf2   #compression: gzip\cf3 \
\
\cf2   # Set the compression level. Currently only gzip provides a compression level\cf3 \
\cf2   # between 0 and 9. The default value is chosen by the compression algorithm.\cf3 \
\cf2   #compression_level: 4\cf3 \
\
\cf2   # The maximum permitted size of JSON-encoded messages. Bigger messages will be\cf3 \
\cf2   # dropped. The default value is 1000000 (bytes). This value should be equal to\cf3 \
\cf2   # or less than the broker's message.max.bytes.\cf3 \
\cf2   #max_message_bytes: 1000000\cf3 \
\
\cf2   # The ACK reliability level required from broker. 0=no response, 1=wait for\cf3 \
\cf2   # local commit, -1=wait for all replicas to commit. The default is 1.  Note:\cf3 \
\cf2   # If set to 0, no ACKs are returned by Kafka. Messages might be lost silently\cf3 \
\cf2   # on error.\cf3 \
\cf2   #required_acks: 1\cf3 \
\
\cf2   # The configurable ClientID used for logging, debugging, and auditing\cf3 \
\cf2   # purposes.  The default is "beats".\cf3 \
\cf2   #client_id: beats\cf3 \
\
\cf2   # Enable SSL support. SSL is automatically enabled if any SSL setting is set.\cf3 \
\cf2   #ssl.enabled: true\cf3 \
\
\cf2   # Optional SSL configuration options. SSL is off by default.\cf3 \
\cf2   # List of root certificates for HTTPS server verifications\cf3 \
\cf2   #ssl.certificate_authorities: ["/etc/pki/root/ca.pem"]\cf3 \
\
\cf2   # Configure SSL verification mode. If `none` is configured, all server hosts\cf3 \
\cf2   # and certificates will be accepted. In this mode, SSL based connections are\cf3 \
\cf2   # susceptible to man-in-the-middle attacks. Use only for testing. Default is\cf3 \
\cf2   # `full`.\cf3 \
\cf2   #ssl.verification_mode: full\cf3 \
\
\cf2   # List of supported/valid TLS versions. By default all TLS versions from 1.0 up to#ssl.supported_protocols: [TLSv1.0, TLSv1.1, TLSv1.2]\cf3 \
\
\cf2   # Certificate for SSL client authentication\cf3 \
\cf2   #ssl.certificate: "/etc/pki/client/cert.pem"\cf3 \
\
\cf2   # Client Certificate Key\cf3 \
\cf2   #ssl.key: "/etc/pki/client/cert.key"\cf3 \
\
\cf2   # Optional passphrase for decrypting the Certificate Key.\cf3 \
\cf2   #ssl.key_passphrase: ''\cf3 \
\
\cf2   # Configure cipher suites to be used for SSL connections\cf3 \
\cf2   #ssl.cipher_suites: []\cf3 \
\
\cf2   # Configure curve types for ECDHE-based cipher suites\cf3 \
\cf2   #ssl.curve_types: []\cf3 \
\
\cf2   # Configure what types of renegotiation are supported. Valid options are\cf3 \
\cf2   # never, once, and freely. Default is never.\cf3 \
\cf2   #ssl.renegotiation: never\cf3 \
\
\cf2 #------------------------------- Redis output ----------------------------------\cf3 \
\cf2 #output.redis:\cf3 \
\cf2   # Boolean flag to enable or disable the output module.\cf3 \
\cf2   #enabled: true\cf3 \
\
\cf2   # Configure JSON encoding\cf3 \
\cf2   #codec.json:\cf3 \
\cf2     # Pretty print json event\cf3 \
\cf2     #pretty: false\cf3 \
\
\cf2     # Configure escaping HTML symbols in strings.\cf3 \
\cf2     #escape_html: false\cf3 \
\
\cf2   # The list of Redis servers to connect to. If load-balancing is enabled, the\cf3 \
\cf2   # events are distributed to the servers in the list. If one server becomes\cf3 \
\cf2   # unreachable, the events are distributed to the reachable servers only.\cf3 \
\cf2   #hosts: ["localhost:6379"]\cf3 \
\
\cf2   # The name of the Redis list or channel the events are published to. The\cf3 \
\cf2   # default is filebeat.\cf3 \
\cf2   #key: filebeat\cf3 \
\
\cf2   # The password to authenticate to Redis with. The default is no authentication.\cf3 \
\cf2   #password:\cf3 \
\cf2 # The Redis database number where the events are published. The default is 0.\cf3 \
\cf2   #db: 0\cf3 \
\
\cf2   # The Redis data type to use for publishing events. If the data type is list,\cf3 \
\cf2   # the Redis RPUSH command is used. If the data type is channel, the Redis\cf3 \
\cf2   # PUBLISH command is used. The default value is list.\cf3 \
\cf2   #datatype: list\cf3 \
\
\cf2   # The number of workers to use for each host configured to publish events to\cf3 \
\cf2   # Redis. Use this setting along with the loadbalance option. For example, if\cf3 \
\cf2   # you have 2 hosts and 3 workers, in total 6 workers are started (3 for each\cf3 \
\cf2   # host).\cf3 \
\cf2   #worker: 1\cf3 \
\
\cf2   # If set to true and multiple hosts or workers are configured, the output\cf3 \
\cf2   # plugin load balances published events onto all Redis hosts. If set to false,\cf3 \
\cf2   # the output plugin sends all events to only one host (determined at random)\cf3 \
\cf2   # and will switch to another host if the currently selected one becomes\cf3 \
\cf2   # unreachable. The default value is true.\cf3 \
\cf2   #loadbalance: true\cf3 \
\
\cf2   # The Redis connection timeout in seconds. The default is 5 seconds.\cf3 \
\cf2   #timeout: 5s\cf3 \
\
\cf2   # The number of times to retry publishing an event after a publishing failure.\cf3 \
\cf2   # After the specified number of retries, the events are typically dropped.\cf3 \
\cf2   # Some Beats, such as Filebeat, ignore the max_retries setting and retry until\cf3 \
\cf2   # all events are published. Set max_retries to a value less than 0 to retry\cf3 \
\cf2   # until all events are published. The default is 3.\cf3 \
\cf2   #max_retries: 3\cf3 \
\
\cf2   # The number of seconds to wait before trying to reconnect to Redis\cf3 \
\cf2   # after a network error. After waiting backoff.init seconds, the Beat\cf3 \
\cf2   # tries to reconnect. If the attempt fails, the backoff timer is increased\cf3 \
\cf2   # exponentially up to backoff.max. After a successful connection, the backoff\cf3 \
\cf2   # timer is reset. The default is 1s.\cf3 \
\cf2   #backoff.init: 1s\cf3 \
\
\cf2   # The maximum number of seconds to wait before attempting to connect to\cf3 \
\cf2   # Redis after a network error. The default is 60s.\cf3 \
\cf2   #backoff.max: 60s\cf3 \
\
\cf2   # The maximum number of events to bulk in a single Redis request or pipeline.\cf3 \
\cf2   # The default is 2048.\cf3 \
\cf2 #bulk_max_size: 2048\cf3 \
\
\cf2   # The URL of the SOCKS5 proxy to use when connecting to the Redis servers. The\cf3 \
\cf2   # value must be a URL with a scheme of socks5://.\cf3 \
\cf2   #proxy_url:\cf3 \
\
\cf2   # This option determines whether Redis hostnames are resolved locally when\cf3 \
\cf2   # using a proxy. The default value is false, which means that name resolution\cf3 \
\cf2   # occurs on the proxy server.\cf3 \
\cf2   #proxy_use_local_resolver: false\cf3 \
\
\cf2   # Enable SSL support. SSL is automatically enabled, if any SSL setting is set.\cf3 \
\cf2   #ssl.enabled: true\cf3 \
\
\cf2   # Configure SSL verification mode. If `none` is configured, all server hosts\cf3 \
\cf2   # and certificates will be accepted. In this mode, SSL based connections are\cf3 \
\cf2   # susceptible to man-in-the-middle attacks. Use only for testing. Default is\cf3 \
\cf2   # `full`.\cf3 \
\cf2   #ssl.verification_mode: full\cf3 \
\
\cf2   # List of supported/valid TLS versions. By default all TLS versions 1.0 up to\cf3 \
\cf2   # 1.2 are enabled.\cf3 \
\cf2   #ssl.supported_protocols: [TLSv1.0, TLSv1.1, TLSv1.2]\cf3 \
\
\cf2   # Optional SSL configuration options. SSL is off by default.\cf3 \
\cf2   # List of root certificates for HTTPS server verifications\cf3 \
\cf2   #ssl.certificate_authorities: ["/etc/pki/root/ca.pem"]\cf3 \
\
\cf2   # Certificate for SSL client authentication\cf3 \
\cf2   #ssl.certificate: "/etc/pki/client/cert.pem"\cf3 \
\
\cf2   # Client Certificate Key\cf3 \
\cf2   #ssl.key: "/etc/pki/client/cert.key"\cf3 \
\
\cf2   # Optional passphrase for decrypting the Certificate Key.\cf3 \
\cf2   #ssl.key_passphrase: ''\cf3 \
\
\cf2   # Configure cipher suites to be used for SSL connections\cf3 \
\cf2   #ssl.cipher_suites: []\cf3 \
\
\cf2   # Configure curve types for ECDHE based cipher suites\cf3 \
\cf2   #ssl.curve_types: []\cf3 \
\
\cf2   # Configure what types of renegotiation are supported. Valid options are\cf3 \
\cf2   # never, once, and freely. Default is never.\cf3 \
\cf2 #ssl.renegotiation: never\cf3 \
\
\cf2 #------------------------------- File output -----------------------------------\cf3 \
\cf2 #output.file:\cf3 \
\cf2   # Boolean flag to enable or disable the output module.\cf3 \
\cf2   #enabled: true\cf3 \
\
\cf2   # Configure JSON encoding\cf3 \
\cf2   #codec.json:\cf3 \
\cf2     # Pretty-print JSON event\cf3 \
\cf2     #pretty: false\cf3 \
\
\cf2     # Configure escaping HTML symbols in strings.\cf3 \
\cf2     #escape_html: false\cf3 \
\
\cf2   # Path to the directory where to save the generated files. The option is\cf3 \
\cf2   # mandatory.\cf3 \
\cf2   #path: "/tmp/filebeat"\cf3 \
\
\cf2   # Name of the generated files. The default is `filebeat` and it generates\cf3 \
\cf2   # files: `filebeat`, `filebeat.1`, `filebeat.2`, etc.\cf3 \
\cf2   #filename: filebeat\cf3 \
\
\cf2   # Maximum size in kilobytes of each file. When this size is reached, and on\cf3 \
\cf2   # every Filebeat restart, the files are rotated. The default value is 10240\cf3 \
\cf2   # kB.\cf3 \
\cf2   #rotate_every_kb: 10000\cf3 \
\
\cf2   # Maximum number of files under path. When this number of files is reached,\cf3 \
\cf2   # the oldest file is deleted and the rest are shifted from last to first. The\cf3 \
\cf2   # default is 7 files.\cf3 \
\cf2   #number_of_files: 7\cf3 \
\
\cf2   # Permissions to use for file creation. The default is 0600.\cf3 \
\cf2   #permissions: 0600\cf3 \
\
