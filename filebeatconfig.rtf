\cf2 #================================= Paths ======================================\cf3 \
\
\cf2 # The home path for the Filebeat installation. This is the default base path\cf3 \
\cf2 # for all other path settings and for miscellaneous files that come with the\cf3 \
\cf2 # distribution (for example, the sample dashboards).\cf3 \
\cf2 # If not set by a CLI flag or in the configuration file, the default for the\cf3 \
\cf2 # home path is the location of the binary.\cf3 \
\cf2 #path.home:\cf3 \
\
\cf2 # The configuration path for the Filebeat installation. This is the default\cf3 \
\cf2 # base path for configuration files, including the main YAML configuration file\cf3 \
\cf2 # and the Elasticsearch template file. If not set by a CLI flag or in the\cf3 \
\cf2 # configuration file, the default for the configuration path is the home path.\cf3 \
\cf2 #path.config: $\{path.home\}\cf3 \
\
\cf2 # The data path for the Filebeat installation. This is the default base path\cf3 \
\cf2 # for all the files in which Filebeat needs to store its data. If not set by a\cf3 \
\cf2 # CLI flag or in the configuration file, the default for the data path is a data\cf3 \
\cf2 # subdirectory inside the home path.\cf3 \
\cf2 #path.data: $\{path.home\}/data\cf3 \
\
\cf2 # The logs path for a Filebeat installation. This is the default location for\cf3 \
\cf2 # the Beat's log files. If not set by a CLI flag or in the configuration file,\cf3 \
\cf2 # the default for the logs path is a logs subdirectory inside the home path.\cf3 \
\cf2 #path.logs: $\{path.home\}/logs\cf3 \
\
\cf2 #================================ Keystore ==========================================\cf3 \
\cf2 # Location of the Keystore containing the keys and their sensitive values.\cf3 \
\cf2 #keystore.path: "$\{path.config\}/beats.keystore"\cf3 \
\
\cf2 #============================== Dashboards =====================================\cf3 \
\cf2 # These settings control loading the sample dashboards to the Kibana index. Loading\cf3 \
\cf2 # the dashboards are disabled by default and can be enabled either by setting the\cf3 \
\cf2 # options here, or by using the `-setup` CLI flag or the `setup` command.\cf3 \
\cf2 #setup.dashboards.enabled: false\cf3 \
\
\cf2 # The directory from where to read the dashboards. The default is the `kibana`\cf3 \
\cf2 # folder in the home path.\cf3 \
\cf2 #setup.dashboards.directory: $\{path.home\}/kibana\cf3 \
\
\cf2 # The URL from where to download the dashboards archive. It is used instead of\cf3 \
\cf2 # the directory if it has a value.\cf3 \
\cf2 #setup.dashboards.url:\cf3 \
\cf2 # The file archive (zip file) from where to read the dashboards. It is used instead\cf3 \
\cf2 # of the directory when it has a value.\cf3 \
\cf2 #setup.dashboards.file:\cf3 \
\
\cf2 # In case the archive contains the dashboards from multiple Beats, this lets you\cf3 \
\cf2 # select which one to load. You can load all the dashboards in the archive by\cf3 \
\cf2 # setting this to the empty string.\cf3 \
\cf2 #setup.dashboards.beat: filebeat\cf3 \
\
\cf2 # The name of the Kibana index to use for setting the configuration. Default is ".kibana"\cf3 \
\cf2 #setup.dashboards.kibana_index: .kibana\cf3 \
\
\cf2 # The Elasticsearch index name. This overwrites the index name defined in the\cf3 \
\cf2 # dashboards and index pattern. Example: testbeat-*\cf3 \
\cf2 #setup.dashboards.index:\cf3 \
\
\cf2 # Always use the Kibana API for loading the dashboards instead of autodetecting\cf3 \
\cf2 # how to install the dashboards by first querying Elasticsearch.\cf3 \
\cf2 #setup.dashboards.always_kibana: false\cf3 \
\
\cf2 # If true and Kibana is not reachable at the time when dashboards are loaded,\cf3 \
\cf2 # it will retry to reconnect to Kibana instead of exiting with an error.\cf3 \
\cf2 #setup.dashboards.retry.enabled: false\cf3 \
\
\cf2 # Duration interval between Kibana connection retries.\cf3 \
\cf2 #setup.dashboards.retry.interval: 1s\cf3 \
\
\cf2 # Maximum number of retries before exiting with an error, 0 for unlimited retrying.\cf3 \
\cf2 #setup.dashboards.retry.maximum: 0\cf3 \
\
\
\cf2 #============================== Template =====================================\cf3 \
\
\cf2 # A template is used to set the mapping in Elasticsearch\cf3 \
\cf2 # By default template loading is enabled and the template is loaded.\cf3 \
\cf2 # These settings can be adjusted to load your own template or overwrite existing ones.\cf3 \
\
\cf2 # Set to false to disable template loading.\cf3 \
\cf2 #setup.template.enabled: true\cf3 \
\
\cf2 # Template name. By default the template name is "filebeat-%\{[agent.version]\}"\cf3 \
\cf2 # The template name and pattern has to be set in case the Elasticsearch index pattern is modified.setup.template.name: "filebeat-%\{[agent.version]\}"\cf3 \
\
\cf2 # Template pattern. By default the template pattern is "-%\{[agent.version]\}-*" to apply to the default index settings.\cf3 \
\cf2 # The first part is the version of the beat and then -* is used to match all daily indices.\cf3 \
\cf2 # The template name and pattern has to be set in case the Elasticsearch index pattern is modified.\cf3 \
\cf2 #setup.template.pattern: "filebeat-%\{[agent.version]\}-*"\cf3 \
\
\cf2 # Path to fields.yml file to generate the template\cf3 \
\cf2 #setup.template.fields: "$\{path.config\}/fields.yml"\cf3 \
\
\cf2 # A list of fields to be added to the template and Kibana index pattern. Also\cf3 \
\cf2 # specify setup.template.overwrite: true to overwrite the existing template.\cf3 \
\cf2 # This setting is experimental.\cf3 \
\cf2 #setup.template.append_fields:\cf3 \
\cf2 #- name: field_name\cf3 \
\cf2 #  type: field_type\cf3 \
\
\cf2 # Enable JSON template loading. If this is enabled, the fields.yml is ignored.\cf3 \
\cf2 #setup.template.json.enabled: false\cf3 \
\
\cf2 # Path to the JSON template file\cf3 \
\cf2 #setup.template.json.path: "$\{path.config\}/template.json"\cf3 \
\
\cf2 # Name under which the template is stored in Elasticsearch\cf3 \
\cf2 #setup.template.json.name: ""\cf3 \
\
\cf2 # Overwrite existing template\cf3 \
\cf2 #setup.template.overwrite: false\cf3 \
\
\cf2 # Elasticsearch template settings\cf3 \
setup.template.settings:\
\
\cf2   # A dictionary of settings to place into the settings.index dictionary\cf3 \
\cf2   # of the Elasticsearch template. For more details, please check\cf3 \
\cf2   # https://www.elastic.co/guide/en/elasticsearch/reference/current/mapping.html\cf3 \
\cf2   #index:\cf3 \
\cf2     #number_of_shards: 1\cf3 \
\cf2     #codec: best_compression\cf3 \
\cf2     #number_of_routing_shards: 30\cf3 \
\
\cf2   # A dictionary of settings for the _source field. For more details, please check\cf3 \
\cf2   # https://www.elastic.co/guide/en/elasticsearch/reference/current/mapping-source-field.html\cf3 \
\cf2   #_source:\cf3 \
\cf2     #enabled: false\cf3 \
\cf2 #============================== Setup ILM =====================================\cf3 \
\
\cf2 # Configure index lifecycle management (ILM). These settings create a write\cf3 \
\cf2 # alias and add additional settings to the index template. When ILM is enabled,\cf3 \
\cf2 # output.elasticsearch.index is ignored, and the write alias is used to set the\cf3 \
\cf2 # index name.\cf3 \
\
\cf2 # Enable ILM support. Valid values are true, false, and auto. When set to auto\cf3 \
\cf2 # (the default), the Beat uses index lifecycle management when it connects to a\cf3 \
\cf2 # cluster that supports ILM; otherwise, it creates daily indices.\cf3 \
\cf2 #setup.ilm.enabled: auto\cf3 \
\
\cf2 # Set the prefix used in the index lifecycle write alias name. The default alias\cf3 \
\cf2 # name is 'filebeat-%\{[agent.version]\}'.\cf3 \
\cf2 #setup.ilm.rollover_alias: "filebeat"\cf3 \
\
\cf2 # Set the rollover index pattern. The default is "%\{now/d\}-000001".\cf3 \
\cf2 #setup.ilm.pattern: "\{now/d\}-000001"\cf3 \
\
\cf2 # Set the lifecycle policy name. The default policy name is\cf3 \
\cf2 # 'filebeat-%\{[agent.version]\}'.\cf3 \
\cf2 #setup.ilm.policy_name: "mypolicy"\cf3 \
\
\cf2 # The path to a JSON file that contains a lifecycle policy configuration. Used\cf3 \
\cf2 # to load your own lifecycle policy.\cf3 \
\cf2 #setup.ilm.policy_file:\cf3 \
\
\cf2 # Disable the check for an existing lifecycle policy. The default is false. If\cf3 \
\cf2 # you disable this check, set setup.ilm.overwrite: true so the lifecycle policy\cf3 \
\cf2 # can be installed.\cf3 \
\cf2 #setup.ilm.check_exists: false\cf3 \
\
\cf2 # Overwrite the lifecycle policy at startup. The default is false.\cf3 \
\cf2 #setup.ilm.overwrite: false\cf3 \
\
\cf2 #============================== Kibana =====================================\cf3 \
\
\cf2 # Starting with Beats version 6.0.0, the dashboards are loaded via the Kibana API.\cf3 \
\cf2 # This requires a Kibana endpoint configuration.\cf3 \
setup.kibana:\
  host: "10.2.0.4:5601"\
\cf2 # TODO: Change this to the IP address of your ELK server\cf3 \
\cf2   # Kibana Host\cf3 \
\cf2   # Scheme and port can be left out and will be set to the default (http and 5601)\cf3 \
\cf2   # In case you specify and additional path, the scheme is required: http://localhost:5601/path\cf3 \
\cf2 # Kibana Host\cf3 \
\cf2   # Scheme and port can be left out and will be set to the default (http and 5601)\cf3 \
\cf2   # In case you specify and additional path, the scheme is required: http://localhost:5601/path\cf3 \
